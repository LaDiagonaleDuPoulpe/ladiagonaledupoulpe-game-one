; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "WATTest",
"language": "GDScript",
"path": "res://addons/WAT/core/test/test.gd"
} ]
_global_script_class_icons={
"WATTest": ""
}

[WAT]

Test_Directory="res://Tests"
Results_Directory="res://Tests/results/WAT"
Minimize_Window_When_Running_Tests=false
TestStrategy={
"repeat": 1,
"strategy": "RunAll"
}
Tags=PoolStringArray(  )
Display=8

[application]

config/name="la-diagonale-du-poulpe"
run/main_scene="res://Sources/App/Core/Container-Scenes/RootScene.tscn"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )
config/icon="res://icon.png"

[autoload]

DialoxBoxManager="*res://Sources/App/Shared/Services/UI/DialoxBoxManager.cs"
DialogBox="*res://Sources/App/Shared/Scenes/Dialog/DialogBox.tscn"
LoadingScene="*res://Sources/App/Shared/Scenes/LoadingScene/LoadingScene.tscn"
GlobalDataService="*res://Sources/App/Shared/Services/Data/GlobalDataService.cs"
ResourcesSceneLoader="*res://Sources/App/Shared/Services/Resources/ResourcesSceneLoader.cs"

[display]

window/size/width=1980
window/size/height=1024
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "WAT", "mono_debugger" )

[logging]

file_logging/enable_file_logging=true

[mono]

project/auto_update_project=false

[rendering]

vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
