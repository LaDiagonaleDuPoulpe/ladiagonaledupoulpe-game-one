; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "WATTest",
"language": "GDScript",
"path": "res://addons/WAT/core/test/test.gd"
} ]
_global_script_class_icons={
"WATTest": ""
}

[WAT]

Test_Directory="res://tests"
Results_Directory="res://tests/results/WAT"
Minimize_Window_When_Running_Tests=false
TestStrategy={
"repeat": 1,
"strategy": "RunAll"
}
Tags=PoolStringArray(  )
Display=8

[application]

config/name="la-diagonale-du-poulpe"
run/main_scene="res://Sources/App/Game-Scenes/000-Home/Home.tscn"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )

[autoload]

LoadSettings="*res://Sources/App/Core/LoadSettings.cs"

[display]

window/size/resizable=false
window/handheld/orientation="portrait"
window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "WAT", "mono_debugger" )

[gui]

theme/custom_font="res://addons/CustomNode/new_dynamicfont.tres"

[mono]

debugger_agent/wait_timeout=10000

[rendering]

vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
